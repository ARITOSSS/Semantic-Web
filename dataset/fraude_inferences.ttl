@prefix schema: <https://schema.org/> .
@prefix ex: <http://example.org/ns#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbr:  <http://dbpedia.org/resource/> .

schema:customer a rdf:Property ;
    rdfs:range foaf:Person .

schema:memberTier a owl:DatatypeProperty ;
    rdfs:domain foaf:Person ;
    rdfs:range xsd:string .

ex:transactionLevel a owl:DatatypeProperty ;
    rdfs:domain schema:PurchaseAction ;
    rdfs:range xsd:string .

ex:VIPMember a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            foaf:Person
            [ a owl:Restriction ;
              owl:onProperty schema:memberTier ;
              owl:hasValue "VIP"^^xsd:string
            ]
        )
    ] .

ex:VIPTransaction a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            schema:PurchaseAction
            [ a owl:Restriction ;
              owl:onProperty schema:customer ;
              owl:someValuesFrom ex:VIPMember
            ]
        )
    ] ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ex:transactionLevel ;
        owl:hasValue "VIP"^^xsd:string
    ] .


dbr:France owl:sameAs <http://example.org/country/France> .
dbr:United_States_of_America owl:sameAs <http://example.org/country/United%20States> .
dbr:Japan owl:sameAs <http://example.org/country/Japan> .
dbr:Monaco owl:sameAs <http://example.org/country/Monaco> .
dbr:Italy owl:sameAs <http://example.org/country/Italy> .
dbr:Switzerland owl:sameAs <http://example.org/country/Switzerland> .
dbr:United_Arab_Emirates owl:sameAs <http://example.org/country/United%20Arab%20Emirates> .
dbr:Singapore owl:sameAs <http://example.org/country/Singapore> .
dbr:China owl:sameAs <http://example.org/country/China> .
dbr:United_Kingdom owl:sameAs <http://example.org/country/United%20Kingdom> .
dbr:Australia owl:sameAs <http://example.org/country/Australia> .



    